<%- include('./partials/header'); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">

        <label>Title</label>
        <input class="form-control" type="text" name="newTitle" autocomplete="off">

        <label>Post</label>
        <textarea class="form-control" name="newPost" autocomplete="off" rows="3"></textarea>

    </div>
    <button type="submit" class="btn btn-lg">Publish</button>
</form>


<%- include('./partials/footer'); -%>